parent myfiltermodule v00_01_00
defaultqual e15
#
fcldir product_dir job
#
product         version
larsim		v06_42_00
gcc		v4_9_3a

cetbuildtools	v5_04_03	-	only_for_build
end_product_list

  
# e15  - with gcc 4.9.3 and -std=c++14
